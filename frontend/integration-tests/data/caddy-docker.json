{
  "kind": "Pod",
  "apiVersion":"v1",

  "metadata": {
        "name": "caddy-docker",
        "labels": {
                "name": "caddy-docker"
        }
  },
  "spec": {
      "containers": [{
        "name": "caddy-docker-pod",
        "image": "aosqe/caddy-docker-2",
        "ports": [
          {
            "containerPort": 8080
          },
          {
            "containerPort": 8443
          }
        ],
        "resources":{
          "limits":{
            "memory":"340Mi"
          }
        }
      }]
  }
}

