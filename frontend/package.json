    "@types/glob": "7.x",
    "@types/immutable": "3.x",
    "@types/jasmine": "2.8.x",
    "@types/jasminewd2": "2.0.x",
    "@types/jest": "21.x",
    "@types/lodash-es": "4.x",
    "@types/prop-types": "15.5.6",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.x",
    "@types/react-jsonschema-form": "^1.0.8",
    "@types/react-redux": "6.0.2",
    "@types/react-router-dom": "4.2.7",
