{
  "name": "@console/kubevirt-plugin",
  "version": "0.0.0-fixed",
  "description": "Kubevirt - Virtual machines addon for Kubernetes",
  "private": true,
  "scripts": {
    "lint": "yarn --cwd ../.. eslint packages/kubevirt-plugin"
  },
  "dependencies": {
    "@console/internal": "0.0.0-fixed",
    "@console/plugin-sdk": "0.0.0-fixed",
    "@console/shared": "0.0.0-fixed",
    "@console/dev-console": "0.0.0-fixed",
    "@patternfly/react-console": "1.x"
  },
  "consolePlugin": {
    "entry": "src/plugin.tsx",
    "integrationTestSuites": {
      "kubevirt": [
        "integration-tests/tests/vm.setup.scenario.ts",
        "integration-tests/tests/vm.detail.*.scenario.ts",
        "integration-tests/tests/vm.teardown.scenario.ts",
        "integration-tests/tests/vm.tab.*.scenario.ts",
        "integration-tests/tests/vm.action.*.scenario.ts",
        "integration-tests/tests/vm.cloud-init.scenario.ts",
        "integration-tests/tests/vm.resources.scenario.ts",
        "integration-tests/tests/vm.list-filtering.scenario.ts",
        "integration-tests/tests/vm.yaml.scenario.ts",
        "integration-tests/tests/vm.windows-guest-tools.scenario.ts",
        "integration-tests/tests/vm.wizard.scenario.ts",
        "integration-tests/tests/wizard.validation.scenario.ts",
        "integration-tests/tests/vmi.*.scenario.ts",
        "integration-tests/tests/vmtemplate.*.scenario.ts",
        "integration-tests/tests/vm.non-admin.scenario.ts"
      ],
      "tabs": [
        "integration-tests/tests/vm.tab.*.scenario.ts"
      ],
      "details": [
        "integration-tests/tests/vm.setup.scenario.ts",
        "integration-tests/tests/vm.detail.*.scenario.ts",
        "integration-tests/tests/vm.teardown.scenario.ts"
      ],
      "actions": [
        "integration-tests/tests/vm.action.*.scenario.ts"
      ],
      "vmis": [
        "integration-tests/tests/vmi.*.scenario.ts"
      ],
      "vmtemplate": [
        "integration-tests/tests/vmtemplate.*.scenario.ts"
      ],
      "kubevirt-v2v": [
        "integration-tests/v2v/*.scenario.ts"
      ],
      "kubevirt-gating": [
        "integration-tests/tests/example.scenario.ts"
      ]
    }
  }
}
