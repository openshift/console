{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "moduleResolution": "node",
    "sourceMap": true,
    "jsx": "react",
    "allowJs": true,
    "noEmitOnError": true,
    "declaration": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "lib": ["es2016","es2017", "es2017.object", "es2020.promise", "dom"],
    "typeRoots": ["node_modules/@types", "@types"],
    "types": ["node", "sdk"],
  }
}
