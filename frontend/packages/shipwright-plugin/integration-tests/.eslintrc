{
  "env": {
    "cypress/globals": true,
    "node": true
  },
  "extends": ["../../.eslintrc", "plugin:cypress/recommended", "plugin:prettier/recommended"],
  "plugins": ["cypress"],
  "rules": {
    "no-console": "off",
    "no-namespace": "off",
    "no-redeclare": "off",
    "promise/catch-or-return": "off",
    "promise/no-nesting": "off"
  }
}
